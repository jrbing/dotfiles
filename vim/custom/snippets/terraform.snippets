priority -50

################################################################################
#                                     Base                                     #
################################################################################

snippet var "Variable declaration"
variable "${1:name}" {
	description = "${2}"
	default     = "${3}"
}
$0
endsnippet

snippet evar "Empty variable declaration"
variable "${1:name}" {}$0
endsnippet

snippet qvar "Quoted variable"
"\$\{var.$1\}"$0
endsnippet

snippet ivar "Variable insertion" i
\$\{var.$1\}$0
endsnippet

snippet rvar "Resource as a variable" i
\$\{$1.$2\}$0
endsnippet

snippet output "Output"
output "${1}" {
	value = "${2}"
}
$0
endsnippet

snippet arr "Array"
[ $1 ]$0
endsnippet

snippet tags "Tags"
tags {
	Name = "${2:value}"
}
$0
endsnippet

snippet resource "Resource"
resource "${1:type}" "${2:name}" {
	${3}

	tags {
		name = "${4}"
	}
}
$0
endsnippet

snippet provider "Provider"
provider "${1}" {
	${2}
}
$0
endsnippet

snippet data "Data source"
data "${1}" "${2}" {
	${3}
}
$0
endsnippet

snippet module "Module"
module "${1}" {
	${2}
}
$0
endsnippet

################################################################################
#                                 Provisioners                                 #
################################################################################

snippet ssh "SSH Provisioner"
connection {
	type     = "ssh"
	user     = "root"
	password = "${var.root_password}"
}
$0
endsnippet

snippet winrm "WinRM Provisioner"
connection {
	type     = "winrm"
	user     = "Administrator"
	password = "${var.admin_password}"
}
$0
endsnippet

snippet local_exec "Local Exec Provisioner"
provisioner "local-exec" {
	command = "${1}"
}
$0
endsnippet

snippet remote_exec "Remote Exec Provisioner"
provisioner "remote-exec" {
	inline = [
		"${1}"
	]
}
$0
endsnippet

snippet file "File Provisioner"
provisioner "file" {
	source = "${1}"
	destination = "${2}"
}
$0
endsnippet

