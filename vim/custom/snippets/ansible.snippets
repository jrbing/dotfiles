	sefcontext:
		setype: ${2:# SELinux type for the specified target.}
		target: ${3:# Target path (expression).}
		reload: ${4:yes}
		ftype: ${5:a}
		selevel: ${6:# SELinux range for the specified target.}
		seuser: ${7:# SELinux user for the specified target.}
		state: ${8:#present|absent}
$0
