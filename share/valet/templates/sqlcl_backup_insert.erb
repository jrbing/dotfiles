
-- Datafix <%= @datafix_name %>
-- Created on: <%= @date %>

set echo off
set term off
set timing off
set pause off
set trimspool on
set heading off
set sqlformat insert
set feedback off
set serveroutput on
set null ' '
whenever sqlerror exit failure

spool <%= @record %>_backup_data.sql;

select * from <%= @record %>
WHERE ... ;

spool off
