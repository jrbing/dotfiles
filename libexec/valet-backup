#!/usr/bin/env bash
# Usage: valet backup
# Summary: Runs the backup script
# Help: Backup hard drive to the backup server

# Set the script to exit immediately on error
set -e

# Source generic bash functions
source "$DOTFILES/share/lib/bash-functions.sh"

readonly req_progs=(backup)
bincheck "$req_progs"

cd ~/Backup && backup perform -t backup
